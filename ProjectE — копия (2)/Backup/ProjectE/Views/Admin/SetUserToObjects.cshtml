@model ProjectE.DAO.Objects

@{
    ViewBag.Title = "SetUserToObjects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SetUserToObjects</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Objects</legend>

        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.Description)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Description)
            @Html.ValidationMessageFor(model => model.Description)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.TypeID)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.TypeID)
            @Html.ValidationMessageFor(model => model.TypeID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.OwnerID)
        </div>
        <div class="editor-field">
            @Html.DropDownListFor(model => model.OwnerID, (List<SelectListItem>)ViewBag.UserList)
            @Html.ValidationMessageFor(model => model.OwnerID)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
